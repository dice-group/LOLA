#!/usr/bin/env -S gnuplot -c
set term pngcairo size 1600, 1200
set output ARG2
#set title ""
set xlabel "Experts"
set ylabel "Maximum ratio of tokens in the layer across all languages"
set key top left
set palette defined (0 'blue', 15 'red')
unset colorbox
set xrange [0:15]
set yrange [0:0.75]
plot for [col=1:12] ARG1 using 0:col:(col) with linespoints title columnheader dashtype ' . ' linecolor palette, 0.0625 with lines linecolor 0 title "1/16"
